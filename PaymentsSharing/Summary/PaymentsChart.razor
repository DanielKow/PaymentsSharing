@inject PaymentsChartViewModel ViewModel

<MudStack Spacing="0">
    <MudText Typo="Typo.h3">Payments per month</MudText>
    <MudChart ChartType="ChartType.Bar" ChartSeries="@ViewModel.Series.ToList()" XAxisLabels="@ViewModel.XAxisLabels.ToArray()" Width="100%">
        <CustomGraphics>
            <style>
                .mud-chart-line { overflow: visible; }
            </style>
        </CustomGraphics>
    </MudChart>
</MudStack>